// Beautiful code is simple, but it is also able to get what you want out of your cod the way you want.
// The code is for a ellipse in the top left corner and a rectangle in the bottom right corner

void setup() {
  size(250,250) ;
  //R,G,B
  fill(50,150,200) ;
  background(255) ;
}
  void draw() {
      //X , Y,  X,  Y
  rect(150,150,200,200) ;
  ellipse(50,50,100,100) ;
}
  
